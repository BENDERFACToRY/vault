{
  "name": "vault",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.42",
    "@sveltejs/kit": "^1.0.0-next.148",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.42.1",
    "svelte-check": "^2.2.4",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@apollo/client": "^3.4.7",
    "@fontsource/material-icons-two-tone": "^4.4.5",
    "@fontsource/maven-pro": "^4.4.5",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "formdata-node": "^3.6.4",
    "graphql": "^15.5.1",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.12.5",
    "graphql-ws": "^5.3.0",
    "husky": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "subscriptions-transport-ws": "^0.9.19",
    "svelte-apollo": "npm:@dxlb/svelte-apollo@^0.4.3"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": "eslint --cache --fix",
    "*.{js,ts,css,md,svelte}": "prettier --check"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
