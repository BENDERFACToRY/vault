{
  "name": "vault",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build --verbose",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "houdini": "houdini generate --pull-schema --pull-header x-hasura-admin-secret=secret"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.49",
    "@sveltejs/kit": "^1.0.0-next.192",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "houdini-preprocess": "^0.11.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.4.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/material-icons-two-tone": "^4.4.5",
    "@fontsource/maven-pro": "^4.4.5",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "formdata-node": "^4.3.0",
    "graphql": "^15.7.2",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.12.5",
    "graphql-ws": "^5.3.0",
    "jsonwebtoken": "^8.5.1"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": "eslint --cache --fix",
    "*.{js,ts,css,md,svelte}": "prettier --check"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
